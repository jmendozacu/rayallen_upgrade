<!-- acl for simple google shopping. Visible in te acl treeview of user group rights -->
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../lib/internal/Magento/Framework/Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Wyomind_DataFeedManager::main" title="Data Feed Manager" sortOrder="10">
                    <resource id="Wyomind_DataFeedManager::feeds" title="Manage data feeds" sortOrder="10" />
                    <resource id="Wyomind_DataFeedManager::datafeedmanager_config" title="Manage module configuration" sortOrder="20" />
                </resource>
            </resource>
        </resources>
    </acl>
</config>
